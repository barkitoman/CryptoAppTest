# Environment Variables Example
# Copy this file to .env.development or .env.production and fill in the values

# API URLs
EXPO_PUBLIC_COINMARKETCAP_API_BASE=https://pro-api.coinmarketcap.com/v1
EXPO_PUBLIC_COINCAP_WS_URL=wss://ws.coincap.io/prices

# API Keys
EXPO_PUBLIC_COINMARKETCAP_API_KEY=YOUR_API_KEY_HERE

# Feature Flags (true/false)
EXPO_PUBLIC_ENABLE_CMC=true
EXPO_PUBLIC_ENABLE_WEBSOCKET=true
EXPO_PUBLIC_ENABLE_CHARTS=true
